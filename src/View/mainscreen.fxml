<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="345.0" prefWidth="400.0" style="-fx-border-color: teal; -fx-border-width: 3;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.mainscreencontroller">
    <children>
        <Button fx:id="viewCustomersButton" layoutX="118.0" layoutY="122.0" mnemonicParsing="false" onAction="#onActionViewCustomers" prefHeight="30.0" prefWidth="164.0" style="-fx-border-color: teal; -fx-border-width: 2;" text="CUSTOMERS">
            <font>
                <Font name="Cochin" size="16.0" />
            </font>
        </Button>
        <Label fx:id="schedulingSystemLabel" layoutX="65.0" layoutY="20.0" opacity="0.35" prefWidth="320.0" text="SCHEDULING SYSTEM">
            <font>
                <Font name="Cochin" size="27.0" />
            </font>
        </Label>
        <Button fx:id="viewReportsButton" layoutX="133.0" layoutY="223.0" mnemonicParsing="false" onAction="#onActionViewReports" prefHeight="30.0" prefWidth="138.0" style="-fx-border-color: teal; -fx-border-width: 2;" text="REPORTS">
            <font>
                <Font name="Cochin" size="16.0" />
            </font>
        </Button>
        <Button fx:id="viewAppointmentsButton" layoutX="121.0" layoutY="173.0" mnemonicParsing="false" onAction="#onActionViewAppointments" prefHeight="39.0" prefWidth="164.0" style="-fx-border-color: teal; -fx-border-width: 2;" text="APPOINTMENTS">
            <font>
                <Font name="Cochin" size="16.0" />
            </font>
        </Button>
        <Button fx:id="logoutButton" layoutX="156.0" layoutY="275.0" mnemonicParsing="false" onAction="#onActionLogout" prefHeight="30.0" prefWidth="88.0" style="-fx-border-color: teal; -fx-border-width: 2;" text="LOGOUT">
            <font>
                <Font name="Cochin" size="14.0" />
            </font>
        </Button>
        <Label fx:id="mainMenuLabel" layoutX="122.0" layoutY="67.0" prefHeight="40.0" text="MAIN MENU" textFill="#b9252f">
            <font>
                <Font name="Cochin" size="27.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
